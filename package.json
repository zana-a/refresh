{
  "name": "workspace",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "removeDest": "rm -Rf dest && rm -Rf ./jekyll/assets/css",
    "src:build": "node-sass src -o .jekyll/assets/css --indent-width=2 --output-style=expanded",
    "src:serve": "node-sass src -o .jekyll/assets/css --indent-width=2 --output-style=expanded --watch",
    "dev": "npm run removeDest && npm run dev:build && npm run dev:watchSrc",
    "docs:build": "jekyll build --source .jekyll --destination docs",
    "docs:serve": "jekyll serve --source .jekyll --destination docs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "node-sass": "^4.12.0",
    "prettier": "^1.17.1"
  }
}
